<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lala.mapper.HouseMapper">

    <insert id="save" parameterType="com.lala.entity.House" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO house
        VALUES (#{house.title}, #{house.price}, #{house.area}, #{house.room}, #{house.floor}, #{house.totalFloor}, #{house.watchTimes}, #{house..buildYear}, #{house.status}, #{house.createTime}, #{house.lastUpdateTime}, #{house.cityEnName}, #{house.regionEnName}, #{house.cover}, #{house.direction}, #{house.distanceToSubway}, #{house.parlour}, #{house.district}, #{house.adminId}, #{house.bathroom}, #{house.street})
    </insert>
</mapper>